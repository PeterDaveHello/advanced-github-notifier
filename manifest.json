{
    "applications": {
        "gecko": {
            "strict_min_version": "53.0"
        }
    },
    "manifest_version": 2,
    "background": {
        "scripts": [ "scripts/config.js", "scripts/github.js", "scripts/background.js" ]
    },
    "browser_action": {
        "browser_style": true,
        "default_title": "__MSG_actionTitle__",
        "default_popup": "popup.html",
        "theme_icons": [
            {
                "dark": "images/github.svg",
                "light": "images/github-light.svg",
                "size": 16
            }
        ]
    },
    "content_security_policy": "default-src 'self'; connect-src https://api.github.com https://github.com; object-src 'none'",
    "default_locale": "en",
    "description": "__MSG_description__",
    "name": "__MSG_name__",
    "permissions": [
        "identity",
        "notifications",
        "alarms",
        "storage",
        "https://github.com/login/oauth/access_token"
    ],
    "version": "1.2.1",
    "icons": {
        "48": "images/icon-48.png",
        "64": "images/icon-64.png",
        "96": "images/icon-96.png"
    },
    "options_ui": {
        "page": "options.html",
        "browser_style": true
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Alt+G"
            }
        }
    }
}
